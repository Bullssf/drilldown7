{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "drilldown-7-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/drilldown7/Drilldown7WebPart.js",
          "manifest": "./src/webparts/drilldown7/Drilldown7WebPart.manifest.json"
        }
      ]
    },
    "css-chart-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/cssChart/CssChartWebPart.js",
          "manifest": "./src/webparts/cssChart/CssChartWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "Drilldown7WebPartStrings": "lib/webparts/drilldown7/loc/{locale}.js",
    "ControlStrings": "node_modules/@pnp/spfx-controls-react/lib/loc/{locale}.js",
    "PropertyControlStrings": "node_modules/@pnp/spfx-property-controls/lib/loc/{locale}.js",
    "CssChartWebPartStrings": "lib/webparts/cssChart/loc/{locale}.js"
  }
}
